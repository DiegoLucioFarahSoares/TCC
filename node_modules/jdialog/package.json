{
  "name": "jdialog",
  "version": "1.0.10",
  "description": "一个移动端弹窗组件",
  "main": "dist/jdialog.min.js",
  "scripts": {
    "fix": "standard --fix",
    "lint": "standard",
    "test": "standard && jest --config test/jest.config.js --coverage",
    "test-watch": "jest --watch --config test/jest.config.js",
    "docs": "node ./build/docs.js",
    "dev": "webpack-dev-server --inline --progress --open --config build/webpack.dev.config.js",
    "build": "webpack --config build/webpack.prod.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "standard": {
    "ignore": [
      "build/",
      "test/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1120040377/wh-dialog.git"
  },
  "keywords": [
    "dialog",
    "mobile",
    "弹窗",
    "移动端"
  ],
  "author": "王浩",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1120040377/wh-dialog/issues"
  },
  "homepage": "https://github.com/1120040377/wh-dialog#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.12",
    "jest": "^23.3.0",
    "jest-serializer-html": "^5.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.3",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
